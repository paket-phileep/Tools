{
  "nodes": {
    "mylib": {
      "name": "mylib",
      "type": "lib",
      "data": {
        "root": "mylib",
        "targets": {
          "lint": {
            "cache": true,
            "options": {
              "cwd": "mylib",
              "command": "eslint ."
            },
            "inputs": [
              "default",
              "^default",
              "{workspaceRoot}/.eslintrc.json",
              "{projectRoot}/.eslintrc.json",
              "{workspaceRoot}/tools/eslint-rules/**/*",
              {
                "externalDependencies": [
                  "eslint"
                ]
              }
            ],
            "outputs": [
              "{options.outputFile}"
            ],
            "executor": "nx:run-commands",
            "configurations": {}
          },
          "test": {
            "options": {
              "cwd": "mylib",
              "command": "jest"
            },
            "metadata": {
              "technologies": [
                "jest"
              ],
              "description": "Run Jest Tests"
            },
            "cache": true,
            "inputs": [
              "default",
              "^default",
              {
                "externalDependencies": [
                  "jest"
                ]
              }
            ],
            "outputs": [
              "{workspaceRoot}/coverage/mylib"
            ],
            "executor": "nx:run-commands",
            "configurations": {}
          }
        },
        "name": "mylib",
        "$schema": "../node_modules/nx/schemas/project-schema.json",
        "sourceRoot": "mylib/src",
        "projectType": "library",
        "tags": [],
        "implicitDependencies": []
      }
    }
  },
  "externalNodes": {},
  "dependencies": {
    "mylib": []
  },
  "version": "6.0"
}